{
    "extends": "./.svelte-kit/tsconfig.json",
    "compilerOptions": {
        "paths": {
            // Urls
            "$urls": ["src/lib/constants/backend/urls"],
            "$urls/*": ["src/lib/constants/backend/urls/*"],
            // Stores
            "$store": ["src/lib/store"],
            "$store/*": ["src/lib/store/*"],
            // Hooks
            "$hooks": ["src/hooks"],
            "$hooks/*": ["src/hooks/*"],
            // Svelte Defined
            "$lib": ["src/lib"],
            "$lib/*": ["src/lib/*"]
        }
    }
}
